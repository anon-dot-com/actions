{
  "name": "@anon/actions",
  "version": "0.2.0",
  "description": "Run useful automated scripts on the most popular websites with Anon",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./ipAddressUtil": {
      "import": "./dist/ipAddressUtil/index.js",
      "require": "./dist/ipAddressUtil/index.cjs",
      "types": "./dist/ipAddressUtil/index.d.ts"
    },
    "./amazon": {
      "import": "./dist/amazon/index.js",
      "require": "./dist/amazon/index.cjs",
      "types": "./dist/amazon/index.d.ts"
    },
    "./linkedin": {
      "import": "./dist/linkedin/index.js",
      "require": "./dist/linkedin/index.cjs",
      "types": "./dist/linkedin/index.d.ts"
    },
    "./instagram": {
      "import": "./dist/instagram/index.js",
      "require": "./dist/instagram/index.cjs",
      "types": "./dist/instagram/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "just installDependencies"
  },
  "keywords": [],
  "author": "Anonymity Labs",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^20.13.0",
    "rollup": "4.18.0",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "playwright": "^1.43.1",
    "playwright-extra": "^4.3.6"
  },
  "files": [
    "dist/**/*"
  ]
}
